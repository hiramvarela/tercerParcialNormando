{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport ImageButton from './components/ImageButton.vue';\nimport axios from 'axios';\nexport default {\n  name: 'App',\n  data() {\n    return {\n      idJuego: null,\n      equipoPokemon: [],\n      juegoIniciado: false,\n      secuenciaActual: [],\n      secuenciaAMostrar: [],\n      secuenciaAMemorizar: [],\n      juegoFinalizado: false,\n      score: 0,\n      permitirIngresarSecuencia: false\n    };\n  },\n  components: {\n    ImageButton\n  },\n  methods: {\n    async removerPokemon(index) {\n      this.secuenciaAMostrar.splice(index, 1);\n      this.secuenciaActual.splice(index, 1);\n    },\n    async agregarPokemon(pokemon) {\n      if (this.secuenciaActual.length < this.secuenciaAMemorizar.length && this.juegoIniciado && this.permitirIngresarSecuencia) {\n        this.secuenciaAMostrar.push(pokemon);\n        this.secuenciaActual.push(pokemon.identificador);\n      }\n    },\n    async envioSecuencia() {\n      this.permitirIngresarSecuencia = false;\n      const response = await axios.post('https://poke-memo-app-9528044356ae.herokuapp.com/enviarSecuencia', {\n        idJuego: this.idJuego,\n        pokemons: this.secuenciaActual\n      });\n      if (response.data.resultado === 'TERMINADO') {\n        this.juegoFinalizado = true;\n        this.score = response.data.score;\n      } else {\n        this.secuenciaAMostrar = [];\n        this.secuenciaActual = [];\n        this.secuenciaAMemorizar = response.data.pokemonSequence;\n        setTimeout(() => {\n          this.reemplazarSecuenciaMaquina();\n        }, 5000);\n      }\n    },\n    async obtenerEquipoInicial() {\n      const response = await axios.get('https://poke-memo-app-9528044356ae.herokuapp.com/crearJuego');\n      this.equipoPokemon = response.data.equipoInicial;\n      this.idJuego = response.data.idJuego;\n    },\n    async iniciarJuego() {\n      await this.envioSecuencia();\n      this.juegoIniciado = true;\n    },\n    reemplazarSecuenciaMaquina() {\n      const secuenciaDito = [];\n      for (let i = 0; i < this.secuenciaAMemorizar.length; i++) {\n        secuenciaDito.push({\n          \"identificador\": 132,\n          \"nombre\": \"ditto\",\n          \"imagenUrl\": \"https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/132.png\"\n        });\n      }\n      this.secuenciaAMemorizar = secuenciaDito;\n      this.permitirIngresarSecuencia = true;\n    }\n  },\n  mounted() {},\n  created() {\n    this.obtenerEquipoInicial();\n  }\n};","map":{"version":3,"names":["ImageButton","axios","name","data","idJuego","equipoPokemon","juegoIniciado","secuenciaActual","secuenciaAMostrar","secuenciaAMemorizar","juegoFinalizado","score","permitirIngresarSecuencia","components","methods","removerPokemon","index","splice","agregarPokemon","pokemon","length","push","identificador","envioSecuencia","response","post","pokemons","resultado","pokemonSequence","setTimeout","reemplazarSecuenciaMaquina","obtenerEquipoInicial","get","equipoInicial","iniciarJuego","secuenciaDito","i","mounted","created"],"sources":["src/App.vue"],"sourcesContent":["<template>\r\n  <div id=\"app\">\r\n    <img src=\"https://upload.wikimedia.org/wikipedia/commons/9/98/International_Pok%C3%A9mon_logo.svg\" alt=\"Pokémon\" class=\"logo\" style=\"width: 600px; height: auto;\">\r\n    <h2 id=\"Titulo\" v-if=\"!juegoIniciado\">¿Eres el mejor maestro pokemon del mundo?</h2>\r\n    <h3 id=\"Subtitulo\" v-if=\"!juegoIniciado\">Memoriza la mayor cantidad de Pokemons y demuestralo!!</h3>\r\n\r\n    <div  v-if=\"!juegoFinalizado\">\r\n      <h1 id=\"Subtitulo1\" v-if=\"!juegoIniciado\">Equipo elegido para esta ronda:</h1>\r\n      <h1 id=\"Instrucciones\" v-else>Instrucciones:</h1>\r\n    </div>\r\n\r\n    <p id=\"TextoInstrucciones\" v-if=\"juegoIniciado && !juegoFinalizado\">Para enviar la secuencia de Pokemons da click en el Pokemon deseado, una vez que termines tu secuencia enviala dando click en la Pokebola</p>\r\n    <p id=\"TextoInstrucciones1\" v-if=\"juegoIniciado && !juegoFinalizado\">Si te equivocas no pasa nada, solo da click en el pokemon que quieras remover y este será eliminado de la secuencia</p>\r\n\r\n\r\n    <div  v-if=\"!juegoFinalizado\">\r\n      <div v-if=\"equipoPokemon.length===0\">\r\n        <h4>Cargando equipo Pokemon...</h4>\r\n      </div>\r\n      <div class=\"button-container\" v-else>\r\n        <ImageButton\r\n            v-for=\"(objectInfo) in equipoPokemon\"\r\n            :key=\"objectInfo.identificador + 'IT'\"\r\n            @buttonClick=\"agregarPokemon(objectInfo)\"\r\n            :imageSrc=\"objectInfo.imagenUrl\"\r\n        />\r\n      </div>\r\n    </div>\r\n\r\n\r\n    <br v-if=\"!juegoIniciado\">\r\n    <br v-if=\"!juegoIniciado\">\r\n    <button id='Start' class=\"start-button\"\r\n            v-if=\"equipoPokemon.length!==0 && !juegoIniciado\"\r\n            v-on:click=\"iniciarJuego\"\r\n    ></button>\r\n\r\n    <div v-if=\"juegoIniciado\">\r\n      <div v-show=\"secuenciaAMemorizar.length !== secuenciaActual.length\">\r\n        <h1>Secuencia a memorizar:</h1>\r\n        <img v-for=\"(objectInfo, index) in secuenciaAMemorizar\"\r\n             :key=\"objectInfo.identificador + 'SM' + index\"\r\n             :src=\"objectInfo.imagenUrl\"\r\n             style=\"width: 100px; height: auto;\"\r\n        />\r\n      </div>\r\n\r\n      <div v-if=\"!juegoFinalizado\">\r\n        <h1>Secuencia a enviar:</h1>\r\n        <img v-for=\"(objectInfo, index) in secuenciaAMostrar\"\r\n             v-on:click=\"removerPokemon(index)\"\r\n             :key=\"index\"\r\n             :src=\"objectInfo.imagenUrl\"\r\n             style=\"width: 100px; height: auto;\"\r\n        />\r\n        <br>\r\n        <button class=\"play-button\"\r\n                v-show=\"secuenciaAMemorizar.length === secuenciaActual.length\"\r\n                v-on:click=\"envioSecuencia\"\r\n        ></button>\r\n      </div>\r\n      <div v-else>\r\n        <h1>GAME OVER</h1>\r\n        <h2>Puntaje: {{score}}</h2>\r\n      </div>\r\n\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n\r\nimport ImageButton from './components/ImageButton.vue';\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  name: 'App',\r\n  data(){\r\n    return {\r\n      idJuego: null,\r\n      equipoPokemon: [],\r\n      juegoIniciado: false,\r\n      secuenciaActual: [],\r\n      secuenciaAMostrar: [],\r\n      secuenciaAMemorizar: [],\r\n      juegoFinalizado: false,\r\n      score: 0,\r\n      permitirIngresarSecuencia: false\r\n\r\n    }\r\n  },\r\n  components: {\r\n    ImageButton\r\n  },\r\n  methods: {\r\n    async removerPokemon(index){\r\n      this.secuenciaAMostrar.splice(index, 1);\r\n      this.secuenciaActual.splice(index, 1);\r\n    },\r\n    async agregarPokemon(pokemon){\r\n      if ((this.secuenciaActual.length < this.secuenciaAMemorizar.length) && this.juegoIniciado && this.permitirIngresarSecuencia){\r\n        this.secuenciaAMostrar.push(pokemon);\r\n        this.secuenciaActual.push(pokemon.identificador);\r\n      }\r\n    },\r\n    async envioSecuencia(){\r\n      this.permitirIngresarSecuencia = false;\r\n      const response = await axios.post('https://poke-memo-app-9528044356ae.herokuapp.com/enviarSecuencia', {\r\n        idJuego: this.idJuego,\r\n        pokemons: this.secuenciaActual\r\n      });\r\n\r\n      if (response.data.resultado === 'TERMINADO') {\r\n        this.juegoFinalizado = true;\r\n        this.score = response.data.score\r\n      } else {\r\n        this.secuenciaAMostrar = [];\r\n        this.secuenciaActual = [];\r\n        this.secuenciaAMemorizar = response.data.pokemonSequence;\r\n\r\n        setTimeout(() => {\r\n          this.reemplazarSecuenciaMaquina();\r\n        }, 5000)\r\n      }\r\n    },\r\n    async obtenerEquipoInicial (){\r\n      const response = await axios.get('https://poke-memo-app-9528044356ae.herokuapp.com/crearJuego');\r\n\r\n      this.equipoPokemon = response.data.equipoInicial;\r\n      this.idJuego = response.data.idJuego;\r\n    },\r\n    async iniciarJuego(){\r\n      await this.envioSecuencia();\r\n      this.juegoIniciado = true;\r\n    },\r\n    reemplazarSecuenciaMaquina(){\r\n      const secuenciaDito = []\r\n      for (let i = 0; i < this.secuenciaAMemorizar.length; i++) {\r\n        secuenciaDito.push(\r\n            {\r\n              \"identificador\": 132,\r\n              \"nombre\": \"ditto\",\r\n              \"imagenUrl\": \"https://raw.githubusercontent.com/PokeAPI/sprites/master/sprites/pokemon/132.png\"\r\n            },\r\n        )\r\n      }\r\n\r\n      this.secuenciaAMemorizar = secuenciaDito;\r\n      this.permitirIngresarSecuencia = true;\r\n    }\r\n  },\r\n  mounted() {\r\n\r\n  },\r\n  created() {\r\n    this.obtenerEquipoInicial();\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n#app {\r\n  font-family: Avenir, Helvetica, Arial, sans-serif;\r\n  -webkit-font-smoothing: antialiased;\r\n  -moz-osx-font-smoothing: grayscale;\r\n  text-align: center;\r\n  color: #2c3e50;\r\n  margin-top: 60px;\r\n}\r\n.button-container {\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  gap: 20px;\r\n}\r\n\r\n.start-button {\r\n  width: 150px; /* Ajusta el tamaño según sea necesario */\r\n  height: 50px; /* Ajusta el tamaño según sea necesario */\r\n  background-image: url('https://static.vecteezy.com/system/resources/thumbnails/017/522/394/small/button-for-8-bit-games-start-beginning-vector.jpg');\r\n  background-size: cover;\r\n  background-position: center;\r\n  border: none;\r\n  cursor: pointer;\r\n  transition: transform 0.2s;\r\n}\r\n\r\n.play-button {\r\n  width: 150px; /* Ajusta el tamaño según sea necesario */\r\n  height: 150px; /* Ajusta el tamaño según sea necesario */\r\n  background-image: url('https://images-wixmp-ed30a86b8c4ca887773594c2.wixmp.com/f/e69b48c0-de96-496f-b849-5f7dd6287b87/d5hkwnr-a4bfd508-9fec-4cdf-a9ab-8b4f13e8ad22.png?token=eyJ0eXAiOiJKV1QiLCJhbGciOiJIUzI1NiJ9.._cKS7j2fZedTid0G_YVD1eswwHSX7CNs3vHCyQTzFyA');\r\n  background-size: cover;\r\n  background-position: center;\r\n  border: none;\r\n  cursor: pointer;\r\n  transition: transform 0.2s;\r\n}\r\n\r\n.play-button:hover {\r\n  transform: scale(1.1);\r\n}\r\n\r\n\r\n\r\n.start-button:hover {\r\n  transform: scale(1.1);\r\n}\r\n</style>\r\n"],"mappings":";AAwEA,OAAAA,WAAA;AACA,OAAAC,KAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,aAAA;MACAC,aAAA;MACAC,eAAA;MACAC,iBAAA;MACAC,mBAAA;MACAC,eAAA;MACAC,KAAA;MACAC,yBAAA;IAEA;EACA;EACAC,UAAA;IACAb;EACA;EACAc,OAAA;IACA,MAAAC,eAAAC,KAAA;MACA,KAAAR,iBAAA,CAAAS,MAAA,CAAAD,KAAA;MACA,KAAAT,eAAA,CAAAU,MAAA,CAAAD,KAAA;IACA;IACA,MAAAE,eAAAC,OAAA;MACA,SAAAZ,eAAA,CAAAa,MAAA,QAAAX,mBAAA,CAAAW,MAAA,SAAAd,aAAA,SAAAM,yBAAA;QACA,KAAAJ,iBAAA,CAAAa,IAAA,CAAAF,OAAA;QACA,KAAAZ,eAAA,CAAAc,IAAA,CAAAF,OAAA,CAAAG,aAAA;MACA;IACA;IACA,MAAAC,eAAA;MACA,KAAAX,yBAAA;MACA,MAAAY,QAAA,SAAAvB,KAAA,CAAAwB,IAAA;QACArB,OAAA,OAAAA,OAAA;QACAsB,QAAA,OAAAnB;MACA;MAEA,IAAAiB,QAAA,CAAArB,IAAA,CAAAwB,SAAA;QACA,KAAAjB,eAAA;QACA,KAAAC,KAAA,GAAAa,QAAA,CAAArB,IAAA,CAAAQ,KAAA;MACA;QACA,KAAAH,iBAAA;QACA,KAAAD,eAAA;QACA,KAAAE,mBAAA,GAAAe,QAAA,CAAArB,IAAA,CAAAyB,eAAA;QAEAC,UAAA;UACA,KAAAC,0BAAA;QACA;MACA;IACA;IACA,MAAAC,qBAAA;MACA,MAAAP,QAAA,SAAAvB,KAAA,CAAA+B,GAAA;MAEA,KAAA3B,aAAA,GAAAmB,QAAA,CAAArB,IAAA,CAAA8B,aAAA;MACA,KAAA7B,OAAA,GAAAoB,QAAA,CAAArB,IAAA,CAAAC,OAAA;IACA;IACA,MAAA8B,aAAA;MACA,WAAAX,cAAA;MACA,KAAAjB,aAAA;IACA;IACAwB,2BAAA;MACA,MAAAK,aAAA;MACA,SAAAC,CAAA,MAAAA,CAAA,QAAA3B,mBAAA,CAAAW,MAAA,EAAAgB,CAAA;QACAD,aAAA,CAAAd,IAAA,CACA;UACA;UACA;UACA;QACA,CACA;MACA;MAEA,KAAAZ,mBAAA,GAAA0B,aAAA;MACA,KAAAvB,yBAAA;IACA;EACA;EACAyB,QAAA,GAEA;EACAC,QAAA;IACA,KAAAP,oBAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}